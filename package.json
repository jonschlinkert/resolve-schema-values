{
  "name": "resolve-schema-values",
  "description": "Resolve values based on a JSON schema. Supports conditionals and composition. Useful for configuration, preferences, LLM chat completions, etc.",
  "version": "2.3.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/jonschlinkert/resolve-schema-values"
  },
  "author": {
    "name": "Jon Schlinkert",
    "url": "https://github.com/jonschlinkert"
  },
  "scripts": {
    "prepublish": "npx tsup",
    "eslint": "npx eslint --ext .ts .",
    "test": "ts-mocha -r esbuild-register 'test/**/*.ts'",
    "tsup": "npx tsup"
  },
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./merge": {
      "import": "./dist/merge.mjs",
      "require": "./dist/merge.js"
    },
    "./resolve": {
      "import": "./dist/resolve.mjs",
      "require": "./dist/resolve.js"
    }
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "esbuild-register": "^3.5.0",
    "eslint": "^8.57.0",
    "get-value": "^3.0.1",
    "gulp-format-md": "^2.0.0",
    "prettier": "^3.3.3",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "reflinks": [
      "verb"
    ],
    "related": {
      "list": [
        "clone-deep",
        "kind-of"
      ]
    },
    "lint": {
      "reflinks": true
    }
  }
}
